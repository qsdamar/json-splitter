{
  "name": "@qsdamar/json-splitter",
  "version": "1.0.0",
  "description": "Splits JSON file based on children's objects",
  "main": "bin/index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "version-x": "version-x",
    "version-major": "version-x -m",
    "version-minor": "version-x -i",
    "version-patch": "version-x -p"
  },
  "keywords": [
    "json",
    "splitter"
  ],
  "author": "Amar QSD",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.32.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1"
  },
  "bin": {
    "json-split": "./bin/index.js"
  },
  "dependencies": {
    "yargs": "^16.1.0"
  },
  "repository": "github:qsdamar/json-splitter.git",
  "bugs": {
    "url": "https://github.com/qsdamar/json-splitter/issues"
  },
  "homepage": "https://github.com/qsdamar/json-splitter#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
